# Ayphen Jira Frontend Environment Configuration
# Copy this file to .env and update the values as needed

# API Configuration
# For local development, use your local backend URL
VITE_API_URL=http://localhost:8500

# For production deployment (Vercel), use:
# VITE_API_URL=https://ayphen-pm-toll-latest.onrender.com

# Feature Flags (optional)
VITE_ENABLE_AI=true
VITE_ENABLE_REALTIME=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_GAMIFICATION=false
VITE_ENABLE_BLOCKCHAIN=false
VITE_ENABLE_AR_VR=false
